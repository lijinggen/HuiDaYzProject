.container {
  display: flex;
  flex-direction: column;
}

.classify {
  font-size: 26rpx;
  display: flex;
  flex-direction: row;
  width: 90%;
  border-radius: 12rpx;
  background: rgba(221, 221, 221, 0.397);
  margin-top: 30rpx;
  margin-bottom: 20rpx;
  margin: 0 auto;
  padding: 20rpx;
}

.kuaidibangna {
  padding: 25rpx;
  margin: 0 auto;
  margin-top: 20rpx;
  margin-left: -100px;
  width: 100%;
}

.kuaidibangna_locate {
  display: flex;
  flex-direction: row;
  align-items: center;
}

.kuaidibangna_locate image {
  height: 60rpx;
  width: 60rpx;
}

.kuaidibangna_locate_right {
  border-left: 1rpx solid rgba(235, 235, 235, 0.822);
  display: flex;
  flex-direction: column;
  width: 80%;
}

.kuaidibangna_locate_right input {
  padding-left: 15rpx;
  height: 60rpx;
  width: 100%;
  font-size: 25rpx;
  line-height: 60rpx;
}

.kuaidibangna_attribute {
  display: flex;
  flex-direction: row;
  padding-right: 25rpx;
  align-items: center;
}

.kuaidibangna_attribute input {
  margin-left: 20rpx;
  height: 60rpx;
  width: 35%;
  font-size: 25rpx;
  line-height: 60rpx;
  border-bottom: 1rpx solid rgba(235, 235, 235, 0.822);
}

